%svg.logo{ height: "#{size}", width: "#{size}", "shape-rendering" => "geometricPrecision", xmlns:"http://www.w3.org/2000/svg", version:"1.1", "color-rendering" => "optimizeQuality"  }
  %circle.outline.outline--outer{ cx: "#{size/2}", cy: "#{size/2}", r:"#{size/2 - size/20.0}", "stroke-width" => "#{size/40.0}" }
  %circle.outline.outline--inner{ cx: "#{size/2}", cy: "#{size/2}", r:"#{size/2 - size/10.0}", "stroke-width" => "#{size/40.0}" }
  %circle.outline.hole{ cx: "#{size/2 - size/10.0}", cy: "#{size/2 - size/10.0}", r:"#{size/20.0}", "stroke-width" => "#{size/40.0}" }
  %circle.outline.hole{ cx: "#{size/2 + size/10.0}", cy: "#{size/2 - size/10.0}", r:"#{size/20.0}", "stroke-width" => "#{size/40.0}" }
  %circle.outline.hole{ cx: "#{size/2 - size/10.0}", cy: "#{size/2 + size/10.0}", r:"#{size/20.0}", "stroke-width" => "#{size/40.0}" }
  %circle.outline.hole{ cx: "#{size/2 + size/10.0}", cy: "#{size/2 + size/10.0}", r:"#{size/20.0}", "stroke-width" => "#{size/40.0}" }
  %line.cross{ x1: "#{size/2 - size/10.0}", y1: "#{size/2 - size/10.0}" , x2: "#{size/2 + size/10.0}", y2: "#{size/2 + size/10.0}", "stroke-width" => "#{size/20.0}" }
  %line.cross{ x1: "#{size/2 + size/10.0}", y1: "#{size/2 - size/10.0}" , x2: "#{size/2 - size/10.0}", y2: "#{size/2 + size/10.0}", "stroke-width" => "#{size/20.0}" }
